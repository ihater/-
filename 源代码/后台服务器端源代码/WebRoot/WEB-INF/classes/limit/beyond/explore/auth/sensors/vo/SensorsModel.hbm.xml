<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        '-//Hibernate/Hibernate Mapping DTD 3.0//EN'
        'http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd'>
<hibernate-mapping>
    <class name="limit.beyond.explore.auth.sensors.vo.SensorsModel" table="tbl_sensors">
<!--       <class name="vo.Sensors" table="sensors" dynamic-insert="true" dynamic-update="true">
 -->		
		<id name="uuid" column="uuid" type="integer">
			<generator class="native"/>
		</id>
		
		
		
		<property name="temp"/>
		<property name="user" />
		
		<!--  记录时间戳    设置字段：time timestamp CURRENT_TIMESTAMP  -->
		<property name="time" type="timestamp">  
            <column name="time" />  
        </property>  
		 
		<!-- <many-to-one name="user" column="uid" class="user.User" lazy="false"  not-null="true" insert="false" update="false"/> -->
		
    </class>
</hibernate-mapping>
